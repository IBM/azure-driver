
{
  "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "vnet_name": {
      "value": "{{vnet_name}}"
    },
    "vnet_location": {
      "value": "{{vnet_location}}"
    },
    "vnet_enable_ddos_protection": {
      "value": {{vnet_enable_ddos_protection | tojson}}
    },
    "vnet_private_endpoint_network_policies": {
      "value": "{{vnet_private_endpoint_network_policies}}"
    },
    "vnet_private_link_service_network_policies": {
      "value": "{{vnet_private_link_service_network_policies}}"
    },
    "vnet_default_cidr_block": {
      "value": "{{vnet_default_cidr_block}}"
    },
    "vnet_address_space": {
      "value": {{vnet_address_space.split(',') | tojson}}
    }
  }
}